{
  "name": "rune-shooter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint && tsc && vite build",
    "upload": "npm run build && npx dusk-cli@latest upload",
    "lint": "eslint src --ext ts,tsx",
    "deploy": "npm run build && dusk upload --name 'Star Chaser'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dusk-games-sdk": "4.20.2-snapshot.442",
    "eslint-plugin-react-refresh": "^0.4.7",
    "nipplejs": "^0.10.2",
    "toglib": "^1.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-plugin-dusk": "^1.0.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dusk": "^1.0.0",
    "vite-plugin-qrcode": "^0.2.2"
  }
}
